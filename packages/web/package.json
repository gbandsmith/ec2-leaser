{
  "name": "@ec2-leaser/frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "aws-amplify": "^5.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@tanstack/react-query": "^4.0.5"
  },
  "scripts": {
    "dev": "sst bind vite",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage --reporter=vitest-sonar-reporter --outputFile=sonar-report.xml",
    "format": "prettier --write \"./src/**/*.{js,jsx}\"",
    "sonar": "node sonar.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "autoprefixer": "^10.4.2",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "sonarqube-scanner": "^3.0.1",
    "sst": "^2.1.17",
    "tailwindcss": "^3.0.22",
    "vite": "^4.3.6",
    "vitest": "^0.31.0",
    "vitest-sonar-reporter": "^0.4.0"
  }
